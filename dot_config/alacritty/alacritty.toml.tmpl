{{ if eq .chezmoi.os "linux" }}

{{ else if eq .chezmoi.os "darwin" }}

{{ end }}

[terminal.shell]
{{ if eq .chezmoi.os "linux" }}
program = "/usr/bin/zsh"
{{ else if eq .chezmoi.os "darwin" }}
program = "/bin/zsh"
{{ end }}
args = ["-l", "-c", "tmux a -t default || tmux new -s default"]

[window]
opacity = 0.9
decorations = "full"
dynamic_title = true
startup_mode = "Windowed"

[scrolling]
history = 10000
multiplier = 3

